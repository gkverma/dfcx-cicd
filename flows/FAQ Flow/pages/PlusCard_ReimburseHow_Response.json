        "caseContent": [ {
          "message": {
            "text": {
              "text": [ "\u003cspeak\u003eTo be reimbursed for your payment, first change the payment method from Plus Card, to Personal Funds. Then, after you've paid, upload a photo of the itemized receipt in the app. Please do not add any tip to the total amount. The photo of your receipt will be automatically processed to make sure it's clearly legible without blurriness, or glare. and as soon as the photo is approved, you can complete the delivery as normal. Your reimbursement will be reflected, on your Uber Balance in the app within 24 to 48 hours. and you will receive this reimbursement, as part of your next weekly payout.\u003cbreak time=\"750ms\"/\u003e\u003c/speak\u003e" ]
            },
            "languageCode": "en"
          }
        }, {
          "message": {
            "text": {
              "text": [ "\u003cspeak\u003eTo listen to this message again, just say, repeat it. \u003cbreak time=\"700ms\"/\u003e Is there anything else I can help you with today?\u003c/speak\u003e" ]
            },
            "languageCode": "en"
          }
        } ]
      }, {
        "caseContent": [ {
          "message": {
            "text": {
              "text": [ "\u003cspeak\u003eTo be reimbursed for your payment, first change the payment method from Plus Card, to Personal Funds. Then, after you've paid, upload a photo of the itemized receipt in the app. Please do not add any tip to the total amount. The photo of your receipt will be automatically processed to make sure it's clearly legible without blurriness, or glare. and as soon as the photo is approved, you can complete the delivery as normal. Your reimbursement will be reflected, on your Uber Balance in the app within 24 to 48 hours. and you will receive this reimbursement, as part of your next weekly payout.\u003c/speak\u003e" ]
            },
            "languageCode": "en"
          }
        }, {
          "message": {
            "text": {
              "text": [ "\u003cspeak\u003eIs there anything else I can help you with today? Or, if you're done, you can hang up.\u003c/speak\u003e" ]
            },
            "languageCode": "en"
          }
        } ]
      } ]
