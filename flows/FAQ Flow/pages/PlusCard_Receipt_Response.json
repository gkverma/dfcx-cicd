    "conditionalCases": [ {
      "cases": [ {
        "condition": "$session.params.isRepeat != \"yes\"",
        "caseContent": [ {
          "message": {
            "text": {
              "text": [ "\u003cspeak\u003eI'm sorry to hear that you have problem with the receipt. The photo of your receipt, will be automatically processed to make sure it's clearly legible, without blurriness or glare. If the receipt is hard to read, the photo may be rejected, and you'll be asked to retake it. If you've tried taking the photo 2 or 3 times, and it's still not accepted, you can select, Continue Anyway. If you've lost your receipt, please ask the merchant to print you a new one. The receipt upload also may not work, if you have a weak network connection.\u003cbreak time=\"750ms\"/\u003e \u003c/speak\u003e" ]
            },
            "languageCode": "en"
          }
        }, {
          "message": {
            "text": {
              "text": [ "\u003cspeak\u003eTo listen to this message again, just say, repeat it. \u003cbreak time=\"700ms\"/\u003e Is there anything else I can help you with today?\u003c/speak\u003e" ]
            },
            "languageCode": "en"
          }
        } ]
      }, {
        "caseContent": [ {
          "message": {
            "text": {
              "text": [ "\u003cspeak\u003eI'm sorry to hear that you have problem with the receipt. The photo of your receipt, will be automatically processed to make sure it's clearly legible, without blurriness or glare. If the receipt is hard to read, the photo may be rejected, and you'll be asked to retake it. If you've tried taking the photo 2 or 3 times, and it's still not accepted, you can select, Continue Anyway. If you've lost your receipt, please ask the merchant to print you a new one. The receipt upload also may not work, if you have a weak network connection.\u003cbreak time=\"750ms\"/\u003e \u003c/speak\u003e" ]
            },
            "languageCode": "en"
          }
        }, {
          "message": {
            "text": {
              "text": [ "\u003cspeak\u003eIs there anything else I can help you with today? Or, if you're done, you can hang up.\u003c/speak\u003e" ]
            },
            "languageCode": "en"
          }
        } ]
      } ]
    } ]
