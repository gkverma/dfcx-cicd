    "conditionalCases": [ {
      "cases": [ {
        "condition": "$session.params.isRepeat != \"yes\"",
        "caseContent": [ {
          "message": {
            "text": {
              "text": [ "\u003cspeak\u003eI'm sorry that the Plus Card was declined. There are a few reasons why this may have happened. The card may be declined, if you are at the wrong merchant. Or, if the order total is higher than the amount pre-approved by the customer, the Plus Card will be declined. Please do not tip on the order. If the customer approves a substitute item, the item must be of equal, or lesser value to the original item. Even if your card is declined, you can choose to use your own card or cash instead. For more information on how this works, say, reimbursement info.\u003cbreak time=\"750ms\"/\u003e \u003c/speak\u003e" ]
            },
            "languageCode": "en"
          }
        }, {
          "message": {
            "text": {
              "text": [ "\u003cspeak\u003eTo listen to this message again, just say, repeat it. \u003cbreak time=\"700ms\"/\u003e Is there anything else I can help you with today?\u003c/speak\u003e" ]
            },
            "languageCode": "en"
          }
        } ]
      }, {
        "caseContent": [ {
          "message": {
            "text": {
              "text": [ "\u003cspeak\u003eI'm sorry that the Plus Card was declined. There are a few reasons why this may have happened. The card may be declined, if you are at the wrong merchant. Or, if the order total is higher than the amount pre-approved by the customer, the Plus Card will be declined. Please do not tip on the order. If the customer approves a substitute item, the item must be of equal, or lesser value to the original item. Even if your card is declined, you can choose to use your own card or cash instead. For more information on how this works, say, reimbursement info.\u003c/speak\u003e" ]
            },
            "languageCode": "en"
          }
        }, {
          "message": {
            "text": {
              "text": [ "\u003cspeak\u003eIs there anything else I can help you with today? Or, if you're done, you can hang up.\u003c/speak\u003e" ]
            },
            "languageCode": "en"
          }
        } ]
      } ]
    } ]
