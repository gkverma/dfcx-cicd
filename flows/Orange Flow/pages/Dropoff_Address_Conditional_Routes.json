    "triggerFulfillment": {
      "messages": [ {
        "text": {
          "text": [ "\u003cspeak\u003eOkay, drop off issue.\u003c/speak\u003e" ]
        },
        "languageCode": "en"
      } ],
      "setParameterActions": [ {
        "parameter": "zIntent",
        "value": "ZAgent"
      } ]
    },
    "targetFlow": "ZExit",
    "name": "d8a71bf1-a34f-4547-876f-0ba317bd9089"
  }, {
    "condition": "($session.params.pcDeliveryStatus = \"not_arrived_resto\" OR $session.params.pcDeliveryStatus = \"arrived_resto_1\" OR $session.params.pcDeliveryStatus = \"arrived_resto_2\" OR $session.params.pcDeliveryStatus = \"arrived_resto_3\" OR $session.params.pcDeliveryStatus = \"picked_up\") AND $session.params.pcAddressChanged = \"false\"",
    "triggerFulfillment": {
      "setParameterActions": [ {
