    "targetPage": "Dropoff_Address-yes-CO_Response",
    "name": "9004f34f-c273-4021-93e0-10391a5f3b48"
  }, {
    "condition": "($session.params.pcDeliveryStatus = \"arrived_dropoff_1\" OR $session.params.pcDeliveryStatus = \"arrived_dropoff_2\") AND $session.params.pcContainsAlcohol = \"true\"",
    "triggerFulfillment": {
      "setParameterActions": [ {
        "parameter": "intentName",
        "value": "Dropoff_Address-yes-atDropoff-alcohol"
      } ]
    },
    "targetPage": "Dropoff_Address-yes-atDropoff-alcohol_Response",
    "name": "0bd07564-95da-45d2-8b63-dc777be9a353"
  }, {
    "condition": "($session.params.pcDeliveryStatus = \"arrived_dropoff_1\" OR $session.params.pcDeliveryStatus = \"arrived_dropoff_2\") AND $session.params.pcContainsAlcohol = \"false\" AND $session.params.pcInteractionType = \"INTERACTION_TYPE_leave_at_door\"",
    "triggerFulfillment": {
      "setParameterActions": [ {
        "parameter": "intentName",
        "value": "Dropoff_Address-yes-atDropoff-notAlcohol-leaveAtDoor"
      } ]
    },
    "targetPage": "Dropoff_Address-yes-atDropoff-notAlcohol-leaveAtDoor_Response",
    "name": "a4a70c22-a507-4d98-9d28-8658f6bca42d"
  }, {
    "condition": "($session.params.pcDeliveryStatus = \"arrived_dropoff_1\" OR $session.params.pcDeliveryStatus = \"arrived_dropoff_2\") AND $session.params.pcContainsAlcohol = \"false\" AND $session.params.pcInteractionType != \"leave_at_door\"",
    "triggerFulfillment": {
      "setParameterActions": [ {
        "parameter": "intentName",
        "value": "Dropoff_Address-yes-atDropoff-notAlcohol-meetAtDoor"
