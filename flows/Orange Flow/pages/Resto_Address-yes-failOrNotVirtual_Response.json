    "conditionalCases": [ {
      "cases": [ {
        "condition": "$session.params.isRepeat != \"yes\"",
        "caseContent": [ {
          "message": {
            "text": {
              "text": [ "\u003cspeak\u003eI'm sorry you're having trouble finding the store, and I know how frustrating this can be. Please call the store directly for help. Some locations also have multiple businesses sharing the same space. If this is the case, go to the address in your app, and the staff at that location will help you. If you have already called the store, you can say, I already called them.\u003cbreak time=\"750ms\"/\u003e \u003c/speak\u003e" ]
            },
            "languageCode": "en"
          }
        }, {
          "message": {
            "text": {
              "text": [ "\u003cspeak\u003eTo listen to this message again, just say, repeat it. \u003cbreak time=\"700ms\"/\u003e Is there anything else I can help you with today?\u003c/speak\u003e" ]
            },
            "languageCode": "en"
          }
        } ]
      }, {
        "caseContent": [ {
          "message": {
            "text": {
              "text": [ "\u003cspeak\u003eI'm sorry you're having trouble finding the store, and I know how frustrating this can be. Please call the store directly for help. Some locations also have multiple businesses sharing the same space. If this is the case, go to the address in your app, and the staff at that location will help you. If you have already called the store, you can say, I already called them.\u003cbreak time=\"750ms\"/\u003e \u003c/speak\u003e" ]
            },
            "languageCode": "en"
          }
        }, {
          "message": {
            "text": {
              "text": [ "\u003cspeak\u003eIs there anything else I can help you with today? Or, if you're done, you can hang up.\u003c/speak\u003e" ]
            },
            "languageCode": "en"
          }
        } ]
      } ]
    } ]
